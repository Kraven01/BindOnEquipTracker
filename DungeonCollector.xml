<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Script file="DungeonCollector.lua"/>
    <Frame name="DungeonCollectorFrame" hidden="true" parent="UIParent">
        <Size x="400" y="500"/>
        <Anchors>
            <Anchor point="CENTER"/>
        </Anchors>
        <Backdrop bgFile="Interface/DialogFrame/UI-DialogBox-Background">
            <EdgeSize>
                <AbsValue val="16"/>
            </EdgeSize>
        </Backdrop>
        <Frames>
            <Button name="DungeonCollectorCloseButton" inherits="UIPanelButtonTemplate">
                <Size x="80" y="22"/>
                <Anchors>
                    <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="-10" y="10"/>
                </Anchors>
                <Text>
                    <Text value="Close"/>
                </Text>
                <Scripts>
                    <OnClick>
                        self:GetParent():Hide();
                    </OnClick>
                </Scripts>
            </Button>
            <ScrollFrame name="DungeonCollectorScrollFrame" inherits="UIPanelScrollFrameTemplate">
                <Size x="360" y="400"/>
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="20" y="-20"/>
                </Anchors>
                <ScrollChild>
                    <Frame name="DungeonCollectorScrollChild" inherits="BasicFrameTemplateWithInset">
                        <Size x="360" y="400"/>
                    </Frame>
                </ScrollChild>
            </ScrollFrame>
        </Frames>
    </Frame>
     <Button name="DungeonCollectorMinimapButton" parent="Minimap" size="32">
        <Size x="32" y="32"/>
        <Anchors>
            <Anchor point="CENTER" x="75" y="-75"/>
        </Anchors>
        <HighlightTexture file="Interface\Minimap\UI-Minimap-ZoomButton-Highlight"/>
        <Scripts>
            <OnClick>
                DungeonCollector:ToggleFrame();
            </OnClick>
        </Scripts>
    </Button>
</Ui>
